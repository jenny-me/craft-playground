{
  "buttons": ["html","formatting", "bold", "italic", "unorderedlist", "orderedlist", "link", "image", "line"],
  "plugins": ["properties"],
  "formatting": ["p", "h2", "h3", "h4"],
  "formattingAdd": {
    "eyebrow": {
      "title": "Small Header (Eyebrow)",
      "api": "module.block.format",
      "args": {
          "tag": "p",
          "class": "small-header"
      }
    },
    "largetext": {
      "title": "Large Text",
      "api": "module.block.format",
      "args": {
          "tag": "p",
          "class": "largetext"
      }
    },
    "button": {
      "title": "Dark (Primary) Button",
      "api": "module.block.format",
      "args": {
          "tag": "p",
          "class": "button"
      }
    },
    "whiteButton": {
      "title": "White Button",
      "api": "module.block.format",
      "args": {
          "tag": "p",
          "class": "white-button"
      }
    },
    "arrowLink": {
      "title": "Link with Arrow",
      "api": "module.block.format",
      "args": {
          "tag": "p",
          "class": "arrow-link"
      }
    }
  },
  "imagePosition": {
    "left": "caption left",
    "right": "caption right",
    "center": "caption full"
  },
  "linkNewTab": true,
  "toolbarFixed": true
}
