{
  "buttons": ["html","formatting", "bold", "italic", "sup",  "unorderedlist", "orderedlist", "link", "line", "fullscreen"],
  "plugins": ["properties", "fullscreen"],
  "formatting": ["p", "h2", "h3", "h4", "blockquote"],
  "formattingAdd": {
    "eyebrow": {
        "title": "Small Header (Eyebrow)",
        "api": "module.block.format",
        "args": {
            "tag": "p",
            "class": "small-header"
        }
    },
    "largetext": {
      "title": "Large Text",
      "api": "module.block.format",
      "args": {
          "tag": "p",
          "class": "largetext"
      }
    },
    "button": {
      "title": "Dark (Primary) Button",
      "api": "module.block.format",
      "args": {
          "tag": "p",
          "class": "button"
      }
    },
    "whiteButton": {
      "title": "White Button",
      "api": "module.block.format",
      "args": {
          "tag": "p",
          "class": "white-button"
      }
    },
    "arrowLink": {
      "title": "Link with Arrow",
      "api": "module.block.format",
      "args": {
          "tag": "p",
          "class": "arrow-link"
      }
    }
  },
  "linkNewTab": true,
  "toolbarFixed": true,
  "minHeight": "100px"
}
